<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: D75020;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.basdat.controller.signupController">
   <children>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="329.0" spacing="10.0" style="-fx-background-color: D9D9D9;">
               <children>
                  <Label fx:id="titleLabelSU" alignment="CENTER" prefHeight="32.0" prefWidth="363.0" text="CREATE AN ACCOUNT" textAlignment="CENTER" textFill="#f9602a">
                     <font>
                         <Font name="Gilroy-ExtraBold" size="26.0" />
                     </font>
                  </Label>
                  <Label fx:id="emailLabelSU" prefHeight="24.0" prefWidth="60.0" text="email" textAlignment="CENTER" textFill="#f9602a">
                     <font>
                        <Font name="Gilroy-ExtraBold" size="20.0" />
                     </font>
                  </Label>
                  <TextField fx:id="fieldEmailSU" maxWidth="200.0" promptText="email@zaor.com" style="-fx-background-radius: 10;">
                     <font>
                        <Font name="Gilroy-Light" size="12.0" />
                     </font></TextField>
                  <Label fx:id="usernameLabelSU" text="username" textAlignment="CENTER" textFill="#f9602a">
                     <font>
                        <Font name="Gilroy-ExtraBold" size="20.0" />
                     </font>
                  </Label>
                  <TextField fx:id="fieldUsernameSU" maxWidth="200.0" promptText="username" style="-fx-background-radius: 10;">
                     <font>
                        <Font name="Gilroy-Light" size="12.0" />
                     </font></TextField>
                  <Label fx:id="passwordLabelSU" text="password" textAlignment="CENTER" textFill="#f9602a">
                     <font>
                        <Font name="Gilroy-ExtraBold" size="20.0" />
                     </font>
                  </Label>
                  <PasswordField fx:id="fieldPasswordSU" maxWidth="200.0" promptText="password" style="-fx-background-radius: 10;">
